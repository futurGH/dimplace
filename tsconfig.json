{
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noUnusedParameters": true,
    "importsNotUsedAsValues": "error",
    "noEmit": true,
    "jsx": "react-native"
  },
  "extends": "expo/tsconfig.base"
}
