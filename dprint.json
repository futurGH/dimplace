{
  "typescript": {
    "indentWidth": 4,
    "lineWidth": 100,
    "useTabs": true,
    "preferSingleLine": true,
    "arrowFunction.useParentheses": "force",
    "quoteProps": "asNeeded"
  },
  "json": {},
  "includes": ["src/**/*.{ts,tsx,js,jsx,json}", "./*.{ts,tsx,js,jsx,json}"],
  "excludes": [
    "**/*-lock.json",
    "src/gql/**/*"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.78.0.wasm",
    "https://plugins.dprint.dev/json-0.16.0.wasm"
  ]
}
